   $(function() {
	  
	   var man = new Object();
	   
	   man.name = "song";
	   man.age = "50";
	   man.birthdate = new Date(72,9,1).toLocaleDateString();
	   
	   var jumsu = new Object();
	   jumsu.kor = 30;
	   jumsu.eng = 40;
	   jumsu.math = 50;
	   
	   man.jumsu = jumsu;
	   
	   console.log("이름 : " + man.name);
	   console.log("나이 : " + man.age);
	   console.log("생일 : " + man.birthdate);
	   console.log("국어점수 : " + man.jumsu.kor);
	   console.log("영어점수 : " + man.jumsu.eng);
